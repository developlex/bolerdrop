# Non-secret runtime template. Copy to instances/<store-id>/.env and inject runtime values.

# Identity / versioning
STORE_ID=shop-002
SHOP_AGENT_VERSION=0.1.0
DEPLOYMENT_VERSION=local

# Public entrypoints
MAGENTO_BASE_URL=http://localhost:8182
MAGENTO_HTTP_PORT=8182
SHOP_AGENT_PORT=8192
STOREFRONT_BASE_URL=http://localhost:8282
STOREFRONT_PORT=8282

# Magento package/source profile
MAGENTO_SOURCE_MODE=git
MAGENTO_PACKAGE=magento/project-community-edition
MAGENTO_VERSION=
MAGENTO_REPO_PUBLIC_KEY=__SET_AT_RUNTIME__
MAGENTO_REPO_PRIVATE_KEY=__SET_AT_RUNTIME__
MAGENTO_GIT_REPOSITORY=https://github.com/magento/magento2.git
MAGENTO_GIT_REF=2.4-develop

# Magento runtime dependencies
MAGENTO_DB_NAME=magento
MAGENTO_DB_USER=magento
MAGENTO_DB_PASSWORD=__SET_AT_RUNTIME__
MYSQL_ROOT_PASSWORD=__SET_AT_RUNTIME__

# Magento admin bootstrap values
MAGENTO_ADMIN_USER=__SET_AT_RUNTIME__
MAGENTO_ADMIN_PASSWORD=__SET_AT_RUNTIME__
MAGENTO_ADMIN_EMAIL=__SET_AT_RUNTIME__
MAGENTO_ADMIN_FIRSTNAME=__SET_AT_RUNTIME__
MAGENTO_ADMIN_LASTNAME=__SET_AT_RUNTIME__
MAGENTO_LANGUAGE=en_US
MAGENTO_CURRENCY=USD
MAGENTO_TIMEZONE=UTC
MAGENTO_USE_REWRITES=1

# Shop Agent JWT auth
AGENT_JWT_SECRET=__SET_AT_RUNTIME__
AGENT_JWT_ISSUER=__SET_AT_RUNTIME__
AGENT_JWT_AUDIENCE=__SET_AT_RUNTIME__
AGENT_JWT_LEEWAY_SECONDS=5
AGENT_JWT_MAX_TTL_SECONDS=900
